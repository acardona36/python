def sum_of_squares(xs):
    new_list=[]
    for i in xs:
        new_list.append(i*i)
    print(new_list)
    return(sum(new_list))
    
    
        
            
            
    
   
    

def main():
    alist = []
    for i in range (1,5):
        alist.append(i)
    print(alist)
    
    print(sum_of_squares(alist))

main()
    
